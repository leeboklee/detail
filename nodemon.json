{
  "watch": ["app", "components", "lib", "src"],
  "ext": "js,jsx,ts,tsx",
  "ignore": [
    ".next/**/*",
    "node_modules/**/*",
    ".git/**/*",
    "*.log",
    "dist",
    "build",
    "scripts/**/*",
    "test-*",
    "browser-*",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.gif",
    "*.svg",
    "*.ico",
    "*.pdf",
    "*.zip",
    "*.tar",
    "*.gz"
  ],
  "exec": "npx next dev -H :: -p 3900",
  "restartable": "rs",
  "verbose": false,
  "env": {
    "NODE_ENV": "development",
    "PORT": "3900"
  },
  "cwd": "/home/rsvshop/projects/detail",
  "delay": "3000",
  "legacyWatch": false,
  "polling": false,
  "signal": "SIGTERM"
}
