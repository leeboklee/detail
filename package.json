{
  "name": "detailpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run kill-port && next dev -p 34343",
    "dev:fast": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_SKIP_LOCKFILE_CHECK=1 next dev -p 34343 --turbo",
    "dev:debug": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --inspect\" next dev -p 34343",
    "s": "npm run kill-port && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" node scripts/start-server.js",
    "s:clean": "npm run kill-port && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" CLEAN_CACHE=true node scripts/start-server.js",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start -p 34343",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next node_modules/.cache",
    "clean:all": "rimraf .next node_modules/.cache dist coverage test-results playwright-report",
    "reset": "npm run clean && npm install && npm run dev",
    "kill-port": "npx cross-port-killer 34343",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "node --input-type=module prisma/seed.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "postinstall": "prisma generate",
    "predev": "prisma generate",
    "analyze": "cross-env ANALYZE=true npm run build",
    "check-deps": "npm outdated",
    "update-deps": "npm update",
    "cleanup": "node scripts/cleanup_logs.js",
    "monitor-errors": "node scripts/monitor-errors.js",
    "video:cleanup": "node scripts/video-cleanup.js",
    "video:cleanup:schedule": "node scripts/video-cleanup.js --schedule",
    "test:auto": "node scripts/auto-debug-runner.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@heroui/react": "^2.8.1",
    "@heroui/system": "^2.4.17",
    "@heroui/theme": "^2.4.17",
    "@loadable/component": "^5.16.7",
    "@monaco-editor/react": "^4.7.0",
    "@neondatabase/serverless": "^1.0.0",
    "@nextui-org/react": "^2.6.11",
    "@prisma/client": "^6.10.0",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "critters": "^0.0.23",
    "cross-port-killer": "^1.4.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "next": "^14.2.30",
    "node-fetch": "^3.3.2",
    "open": "^10.1.2",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.37",
    "react": "^18.2.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "split-pane-react": "^0.1.3",
    "styled-components": "^6.1.8",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.5",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/lodash": "^4.17.12",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.4.0",
    "backstopjs": "^6.3.1",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.1",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "fs-extra": "^11.3.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.51.1",
    "prisma": "^6.10.0",
    "puppeteer": "^24.11.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
